{
    "name": "innmind/doctrine",
    "type": "library",
    "description": "Declarative approach to eliminate state where not necessary",
    "keywords": ["doctrine", "declarative", "functional programming"],
    "homepage": "http://github.com/innmind/doctrine",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/innmind/doctrine/issues"
    },
    "require": {
        "php": "~7.4|~8.0",
        "innmind/immutable": "^3.6",
        "doctrine/orm": "^2.7",
        "ramsey/uuid": "^4.0",
        "innmind/specification": "^2.0",
        "innmind/reflection": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Doctrine\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Doctrine\\": "tests/",
            "Fixtures\\Innmind\\Doctrine\\": "fixtures/",
            "Properties\\Innmind\\Doctrine\\": "properties/",
            "Example\\Innmind\\Doctrine\\": "example/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/black-box": "~4.10",
        "innmind/coding-standard": "^1.1",
        "scienta/doctrine-json-functions": "^4.3",
        "symfony/cache": "^6.0"
    },
    "suggest": {
        "scienta/doctrine-json-functions": "To be able to search within a json field"
    }
}
